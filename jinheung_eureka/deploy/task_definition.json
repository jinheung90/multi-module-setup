{
  "name": "${var.app_name}-${var.app_environment}-container",
  "image": "aws_ecr_repository.jhc_gateway_ecr_repository.repository_url"
  "cpu": 10,
  "memory": 512,
  "logConfiguration": {
    "logDriver": "awslogs",
    "options" : {
      "awslogs-group": "jhc",
      "awslogs-region" : "ap-northeast-2",
      "awslogs-stream-prefix" : "ecs"
    }
  },
  "links": [],
  "portMappings" : [
    {
      "hostPort": 8080,
      "containerPort": 8080,
      "protocol": "tcp"
    }
  ],
  "secrets": [],
  "essential": true,
  "entryPoint": [],
  "command": [],
  "environment": [],
  "mountPoints": [],
  "volumesFrom": []
}