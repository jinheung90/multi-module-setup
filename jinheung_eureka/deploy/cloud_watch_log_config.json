{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "ec2-user"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [{
          "file_path": "/home/ec2-user/stdout.log",
          "log_group_name": "stdout-log",
          "log_stream_name": "stdout.log",
          "timestamp_format": "%Y-%m-%d %H:%M:%S.%f"
        }, {
          "file_path": "/home/ec2-user/stderr.log",
          "log_group_name": "stderr-log",
          "log_stream_name": "stderr.log",
          "timestamp_format": "%Y-%m-%d %H:%M:%S.%f"
        }]
      }
    }
  }
}